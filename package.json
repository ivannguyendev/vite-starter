{
  "name": "@ivannguyendev/vite-starter",
  "version": "0.0.0",
  "description": "A general-purpose starter template built with Vite and TypeScript.",
  "homepage": "https://github.com/ivannguyendev/vite-starter#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "ivannguyendev",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist *tsbuildinfo",
    "prod:build": "APP_VERSION=$npm_package_version vite build --mode production",
    "build": "APP_VERSION=$npm_package_version vite build",
    "dev": "vite",
    "test": "ava"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ivannguyendev/vite-starter.git"
  },
  "keywords": [
    "vite",
    "starter",
    "template",
    "typescript",
    "javascript"
  ],
  "dependencies": {},
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@types/node": "~20.18.0",
    "ava": "^6.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.26.0",
    "rimraf": "^6.0.0",
    "ts-node": "~8.4",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.0.0",
    "vite": "^4",
    "vite-plugin-dts": "^4.5.4"
  },
  "ava": {
    "typescript": {
      "compile": false
    }
  }
}
